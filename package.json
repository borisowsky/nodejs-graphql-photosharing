{
  "name": "nodejs-graphql-photosharing",
  "version": "1.0.0",
  "main": "./src/main.ts",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently 'yarn start:development' 'yarn generate'",
    "start:development": "ts-node-dev --require tsconfig-paths/register --respawn --transpile-only --watch './src/graphql/schema/schema.graphql' .",
    "start": "ts-node --require tsconfig-paths/register --transpile-only .",
    "prisma": "prisma",
    "generate": "graphql-codegen --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/typescript-resolvers": "1.18.1",
    "@types/faker": "5.1.6",
    "@types/jest": "26.0.20",
    "apollo-server-testing": "2.19.2",
    "concurrently": "5.3.0",
    "faker": "5.3.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "prisma": "2.16.0",
    "ts-jest": "26.5.0"
  },
  "dependencies": {
    "@prisma/client": "2.16.0",
    "@types/bcrypt": "3.0.0",
    "@types/node": "14.14.22",
    "apollo-server": "2.19.2",
    "bcrypt": "5.0.0",
    "dotenv": "8.2.0",
    "graphql": "15.5.0",
    "jsonwebtoken": "8.5.1",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.3",
    "yup": "0.32.8"
  }
}
